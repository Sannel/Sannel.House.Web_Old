{
	"Database" : {
		"DataRepository" : {
			/*
			* Possible values:
			* sqlite
			* sqlserver (Microsoft Sql Server)
			* 
			* Supported when providers supports ef 2.1:
			* mysql
			*
			* Possible Support in the future:
			* mongdb
			* cosmosdb
			*/
			"Provider": "sqlite",
			"ConnectionString": "Data Source=Data/Data.db"
		},
		"SensorRepository" : {
			/*
			* Possible values:
			* sqlite
			* mongdb
			* sqlserver (Microsoft Sql Server)
			* 
			* Supported when providers supports ef 2.1:
			* mysql
			*
			* Possible Support in the future:
			* cosmosdb
			*/
			"Provider": "sqlite",
			/*
			* Only Used if Provider does not match Repository Provider
			*/
			"ConnectionString": ""
		}
	},
	"Jwt": {
		"Key": "SomeRandomString",
		"Issuer": "https://localhost:54443/",
		"ExpireMinutes": 60
	},
	"Logging": {
		"IncludeScopes": false,
		"Debug": {
			"LogLevel": {
				"Default": "Warning"
			}
		},
		"Console": {
			"LogLevel": {
				"Default": "Warning"
			}
		}
	},
	"AllowedHosts": "*"
}
